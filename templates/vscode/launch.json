{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug test_bitcoin",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build-coverage/bin/test_bitcoin",
			"args": [
				"--run_test=script_tests/script_build"
			],
			"sourceFileMap": {
				"${workspaceFolder}/build-coverage/src/test/test": "${workspaceFolder}/src/test",
				"${workspaceFolder}/build-coverage/src": "${workspaceFolder}/src"
			},
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}
